{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"]
		},
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "node",
		"importHelpers": true,
		"isolatedModules": true,
		"noEmit": true,
		"strict": true,
		"allowJs": true,
		"jsx": "preserve",
		"allowSyntheticDefaultImports": true
	}
}